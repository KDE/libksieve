msgstr "البيانات التعريفية موجودة"

#: ksieveui/autocreatescripts/sieveconditions/sieveconditionmetadataexists.cpp:44
#: ksieveui/autocreatescripts/sieveconditions/sieveconditionservermetadataexists.cpp:33
#, kde-format
msgctxt "@label:textbox"
msgid "Annotation:"
msgstr "الشرح:"

#: ksieveui/autocreatescripts/sieveconditions/sieveconditionmetadataexists.cpp:83
#, kde-format
